{
  "name": "eslint-plugin-sc-js",
  "version": "0.0.2",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:1": "rm -rf ./dist",
    "build:2": "swc \"./src\" -d \"./dist\"  --config-file ../../.swcrc",
    "build:3": "mv ./dist/src/* ./dist",
    "build:4": "rm -rf ./dist/src",
    "check-code": "run-p lint spell-check type-check test",
    "dev": "chokidar \"./{src,spec}/**\" -c \"yarn type-check && yarn test\"",
    "fix": "run-p \"fix:*\" -c",
    "fix:eslint": "eslint \"./\" --fix --max-warnings 0",
    "fix:pack": "fixpack",
    "fix:prettier": "prettier \"./*.json\" \"./**/*.json\" --ignore-path ../../.gitignore -w -u",
    "lint": "run-p \"lint:*\" -c",
    "lint:editorconfig-checker": "ec -dry-run",
    "lint:eslint": "eslint \"./\" --max-warnings 0",
    "lint:pack": "fixpack --dryRun",
    "lint:prettier": "prettier \"./*.json\" \"./**/*.json\" --ignore-path ../../.gitignore -c -u",
    "prepare": "run-p \"prepare:*\"",
    "prepare:copy-shared": "node ../../scripts/copy-shared/index.js",
    "spell-check": "cspell lint --no-progress \"./{spec,src}/**/*.ts\" \"./{config,scripts}/**/*.{js,mjs}\" \"./*.{js,mjs}\"",
    "tagging": "node ../../scripts/tagging/index.mjs",
    "test": "jest --coverage",
    "type-check": "tsc"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@stylistic/eslint-plugin": "^2.1.0",
    "@swc/core": "^1.5.25",
    "@swc/jest": "^0.2.36",
    "@tsconfig/strictest": "^2.0.5",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/rule-tester": "^7.12.0",
    "@typescript-eslint/utils": "^7.12.0",
    "chokidar-cli": "^3.0.0",
    "cspell": "^8.8.4",
    "editorconfig-checker": "^5.1.8",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "fixpack": "^4.0.0",
    "jest": "^29.7.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.3.1",
    "strict-check": "workspace:^",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0"
  },
  "packageManager": "yarn@4.2.2"
}
